# Common options
[mysqld]
log_bin
binlog_format=ROW
gtid_mode=ON
log_slave_updates
enforce_gtid_consistency=1
innodb_file_per_table=1
default_storage_engine=innodb
wsrep-cluster-name=my_cluster
wsrep_sst_method=xtrabackup-v2
innodb_autoinc_lock_mode=2
#wsrep-debug=1
log-error-verbosity=3

[sst]
#encrypt=3
#tkey=/home/kennt/dev/data/certs/server.key
#tcert=/home/kennt/dev/data/certs/server.pem
#tca=/home/kennt/dev/data/certs/server.crt
#encrypt=1
#encrypt-algo=AES256
#encrypt-key=A1EDC73815467C083B0869508406637E
wsrep-debug=1

[mysqld.1]
server-id=1
datadir=/home/kennt/dev/pxc/build-bin/node1
log-error=/home/kennt/dev/pxc/build-bin/node1/error.log
#innodb-tmpdir=/home/kennt/dev/pxc/build-bin/innodb_tmpdir1
#innodb-log-group-home-dir=/tmp/node1
